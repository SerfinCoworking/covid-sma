<div id="filterrific_results">
  <div class="panel panel-default">
    <table class="table table-hover table-striped table-condensed">
      <thead>
        <tr>
          <th>Nombre</th>
          <th class="text-center actions-col">Acciones</th>
        </tr>
      </thead>
      <tbody id="sectors">
        <% if @sectors.present? %>
          <%= render @sectors %>
        <%else%>
          No hay sectores cargados.
        <%end%>
      </tbody>
    </table>
  </div>
  <div id="page_links">
    <%= paginate @sectors %>
  </div>
</div>
