<p id="notice"><%= notice %></p>

<h1>Vademecums</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vademecums.each do |vademecum| %>
      <tr>
        <td><%= link_to 'Show', vademecum %></td>
        <td><%= link_to 'Edit', edit_vademecum_path(vademecum) %></td>
        <td><%= link_to 'Destroy', vademecum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vademecum', new_vademecum_path %>
