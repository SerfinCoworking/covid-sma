<%= render 'header' %>

<% if @internal_orders.present? %>

  <%= render 'filter' %>

  <%= render(
    partial: 'internal_orders/list',
    locals: { internal_orders: @internal_orders }
  ) %>
<% else %>
  <%= render(
    partial: 'shared/not_found',
    locals: { object_name: 'pedidos internos entregados' }
  ) %>
<% end %>
