<div id="filterrific_results">
  <div class="panel panel-default">
    <table class="table table-hover table-striped">
      <thead>
        <tr>
          <th><%= filterrific_sorting_link(@filterrific, :solicitante) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :abastecedor) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :estado) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :insumos_solicitados) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :recibido) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :entregado) %></th>
          <th colspan="3">Acciones</th>
        </tr>
      </thead>
      <tbody id=internal_orders>
        <%= render @internal_orders %>
      </tbody>
    </table>
  </div>
  <div id="page_links">
    <%= paginate @internal_order %>
  </div>
</div>
