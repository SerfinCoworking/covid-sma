<%= render 'header' %>

<% if @applicant_orders.present? %>

  <%= render "filter" %>

  <%= render(
    partial: 'internal_orders/applicant_list',
    locals: { applicant_orders: @applicant_orders }
  ) %>
<% else %>
  <%= render(
    partial: 'shared/not_found',
    locals: { object_name: 'pedidos internos recibidos' }
  ) %>
<% end %>