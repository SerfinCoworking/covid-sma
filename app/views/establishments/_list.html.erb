<% if @establishments.present? %>
  <div id="filterrific_results">
    <div class="panel panel-default">
      <table class="table table-hover table-striped table-condensed">
        <thead>
          <tr>
            <th><%= filterrific_sorting_link(@filterrific, :cod) %></th>
            <th><%= filterrific_sorting_link(@filterrific, :nombre) %></th>
            <th><%= filterrific_sorting_link(@filterrific, :sectores) %></th>
            <th><%= filterrific_sorting_link(@filterrific, :usuarios) %></th>
            <th colspan="3">Acciones</th>
          </tr>
        </thead>
        <tbody id="establishments">
          <%= render @establishments %>
        </tbody>
      </table>
    </div>
    <div id="page_links">
      <%= paginate @establishment %>
    </div>
  </div>
<% else %>
  <%= render(
    partial: 'shared/not_found_filter',
    locals: { object_name: 'entregas' }
  ) %>
<% end %>

