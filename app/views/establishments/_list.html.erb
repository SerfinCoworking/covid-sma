<div id="filterrific_results">
  <div class="panel panel-default">
    <table class="table table-hover table-striped table-condensed">
      <thead>
        <tr>
          <th>Código</th>
          <th>Nombre</th>
          <th>Domicilio</th>
          <th>Email</th>
          <th>Teléfono</th>
          <th>Sectores</th>
          <th class="text-center">Acciones</th>
        </tr>
      </thead>
      <tbody id="establishments">
        <% if @establishments.present? %>
          <%= render @establishments %>
        <%else%>
          No hay establecimientos cargados.
        <%end%>
      </tbody>
    </table>
  </div>
  <div id="page_links">
    <%= paginate @establishment %>
  </div>
</div>
