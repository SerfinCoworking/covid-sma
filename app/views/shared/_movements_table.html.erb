<div class="row hidden-print">
  <div class="col-md-12">
    <table class="table table-hover table-condensed">
      <thead>
        <tr>
          <th>Fecha</th>
          <th>Movimiento</th>
          <th>Usuario</th>
          <th>Sector</th>
          <th>Establecimiento</th>
        </tr>
      </thead>
      <tbody>
        <% order.ordering_supply_movements.order(created_at: :desc).each do |mov| %>
          <tr>
            <td><%= mov.created_at.strftime("%d/%m/%y %H:%M") %></td>
            <td><%= mov.action.humanize %></td>
            <td><%= mov.user.full_name %></td>
            <td><%= mov.sector.name %></td>
            <td><%= mov.sector.establishment.name%></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>