<%= render 'header' %>

<% if @supply_lots.present? %>
  <div class="card fixed-custom-card">
    <div class="card-header">
      <%= render 'filter' %>
    </div>
    <div class="card-body fixed-panel-index">
      <%= render(
        partial: 'supply_lots/list',
        locals: { supply_lots: @supply_lots }
      ) %>
    </div>
  </div>
<% else %>
  <%= render(
    partial: 'shared/not_found',
    locals: { object_name: 'lotes en provincia' }
  ) %>
<% end %>