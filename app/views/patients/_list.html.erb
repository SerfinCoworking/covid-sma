<div id="filterrific_results">
  <div class="panel panel-default">
    <table class="table table-hover table-striped">
      <thead>
        <tr>
          <th><%= filterrific_sorting_link(@filterrific, :nombre) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :apellido) %></th>
          <th>DNI</th>
          <th><%= filterrific_sorting_link(@filterrific, :tipo_de_paciente) %></th>
          <th>Cargado</th>
          <th colspan="3">Acciones</th>
        </tr>
      </thead>
      <tbody id=patients>
        <%= render @patients %>
      </tbody>
    </table>
    <%= render 'dialog' %>
  </div>
</div>
<div id="page_links">
    <%= paginate @patient %>
</div>
