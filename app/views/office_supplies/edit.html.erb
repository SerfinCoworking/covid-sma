<%= render 'header' %>

<div class="panel panel-warning">
  <div class="panel-heading">
    <h3 class="panel-title"><%= content_tag(:span, '', class: 'glyphicon glyphicon-edit') %> Editando insumo de oficina
      <%= link_to office_supplies_path, class: 'btn pull-right close-btn' do %>
        <%= content_tag(:span, '', class: 'glyphicon glyphicon-remove') %>
      <% end %>
    </h3>
  </div>
  <div class="panel-body" id="panel-body-new">
    <%= render 'form', office_supply: @office_supply %>
  </div>
  <div class="panel-footer text-right">
    <%= link_to 'Volver', office_supplies_path, class: 'btn' %>
    <button type='submit' name='commit' class='btn btn-success' form='edit_office_supply_<%=@office_supply.id%>' value='Solicitante'>
      <%= content_tag(:span, '', class: 'glyphicon glyphicon-floppy-saved') %> Guardar cambios
    </button>
  </div>
</div>