<div id="filterrific_results">
  <% if @office_supplies.present? %>
    <div class="panel panel-default">
      <table class="table table-hover table-striped">
        <thead>
          <tr>
            <th>Código</th>
            <th>Nombre</th>
            <th>Descripción</th>
            <th>Cantidad</th>
            <th>Estado</th>
            <th>
              Cargado
              <%= content_tag(:span, '', class: 'glyphicon glyphicon-calendar') %>
            </th>
            <th>
              Modificado
              <%= content_tag(:span, '', class: 'glyphicon glyphicon-calendar') %>  
            </th>
            <th colspan="3">Acciones</th>
          </tr>
        </thead>
        <tbody id=office_supplies>
          <%= render @office_supplies %>
        </tbody>
      </table>
    </div>
    <div id="page_links">
      <%= paginate @office_supply %>
    </div>
  <% else %>
    <%= render(
      partial: 'shared/not_found_filter',
      locals: { object_name: 'insumos de oficina' }
    ) %>
  <% end %>
</div>
