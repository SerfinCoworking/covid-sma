<div class="panel panel-default">
  <table class="table table-hover table-striped">
    <thead>
      <tr>
        <th>Medicamento</th>
        <th>Marca</th>
        <th>Cantidad</th>
        <th>Fecha recepción</th>
        <th>Fecha expiración</th>
        <th colspan="3">Acciones</th>
      </tr>
    </thead>

    <tbody id=medications>
      <%= render @medications %>
    </tbody>
  </table>
  <%= render 'dialog' %>
</div>
<%= will_paginate(@medications, remote: true,
  renderer: BootstrapPagination::Rails, previous_label: 'Atras', next_label: 'Siguiente') %>
