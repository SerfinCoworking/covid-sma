<div id="filterrific_results">
  <div class="panel panel-default">
    <table class="table table-hover table-striped">
      <thead>
        <tr>
          <th><%= filterrific_sorting_link(@filterrific, :doctor) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :paciente) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :estado) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :insumos_solicitados) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :recetada) %></th>
          <th><%= filterrific_sorting_link(@filterrific, :dispensada) %></th>
          <th colspan="3">Acciones</th>
        </tr>
      </thead>
      <tbody id=prescriptions>
        <%= render @prescriptions %>
      </tbody>
    </table>
  </div>
  <div id="page_links">
    <%= paginate @prescription %>
  </div>
</div>
