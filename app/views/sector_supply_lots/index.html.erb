<%= render 'header' %>


<% if @sector_supply_lots.present? %>
  <div class="card fixed-custom-card">
    <div class="card-header">
      <%= render 'filter' %>
    </div>
    <div class="card-body fixed-panel-index">
      <%= render(
        partial: 'sector_supply_lots/list',
        locals: { sector_supply_lots: @sector_supply_lots }
      ) %>
    </div>
  </div>
<% else %>
  <%= render(
    partial: 'shared/not_found',
    locals: { object_name: 'lotes en stock' }
  ) %>
<% end %>